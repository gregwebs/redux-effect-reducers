{
  "name": "redux-effect-reducers",
  "version": "1.0.0",
  "description": "Library that enable returning of effects in reducers for redux library",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/redux-effect-reducers.js && NODE_ENV=production webpack src/index.js dist/redux-effect-reducers.min.js",
    "lint": "eslint src test examples",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matystl/redux-effect-reducers.git"
  },
  "keywords": [
    "redux effects elm async-reducers"
  ],
  "author": "matystl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matystl/redux-effect-reducers/issues"
  },
  "homepage": "https://github.com/matystl/redux-effect-reducers",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-loader": "^5.1.4"
  },
  "dependencies": {
    "invariant": "^2.0.0"
  }
}
